void ElectronicsElementSpice::Init(const string & file, string & input, string & output)
{
	SetName(file);
	m_ConstSampling = false; //spice takes everything
	//accepts only fourier space data
	m_SpaceCapability.push_back(Space::FOURIER);
	
}